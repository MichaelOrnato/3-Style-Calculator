                            System.out.println("\nSolution: R' U R U R U R' U' R' U'");
                            break;
                        case "U":
                            System.out.println("Second Target: DF");
                            System.out.println("This cycle is an F-Swap.");
                            System.out.println("\nSolution: [R' F' : [R S' R', F2]]");
                            System.out.println("Or: R' F' R S' R' F2 R S R' F2' F R");
                            break;
                        case "V":
                            System.out.println("Second Target: DR");
                            System.out.println("This cycle cannot be done.");
                            break;
                        case "W":
                            System.out.println("Second Target: DB");
