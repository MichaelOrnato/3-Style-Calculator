                            break;
                        case "G":
                            System.out.println("\nLocation: LD");
                            System.out.println("This cycle cannot be done.");
                            break;
                        case "H":
                            System.out.println("Second Target: LB: ");
                            System.out.println("This cycle is a 4-Mover.");
                            System.out.println("\nSolution: [u L : [E, L2']]");
                            System.out.println("Or: u L E L2' E' L2 L' u'");
                            break;
                        case "J":
                            System.out.println("Second Target: FR");
                            System.out.println("This cycle is a 4-Mover.");
                            System.out.println("\nSolution: [U L' : [E', L2]]");
                            System.out.println("Or: U L' E' L2 E L2' L U'");
                            break;
                        case "K":
                            System.out.println("Second Target: FD");
                            System.out.println("This cycle is a 4-Mover.");
                            System.out.println("\nSolution: [U' L F L' : [S', L2]]");
                            System.out.println("Or: U' L F L' S' L2 S L2' L F' L' U");
                            break;
                        case "L":
                            System.out.println("Second Target: FL");
                            System.out.println("This cycle is an algorithm.");
                            System.out.println("\nSolution: U2' L' U L U L U L' U' L' U");
                            break;
                        case "M":
                            System.out.println("Second Target: RU");
                            System.out.println("This cycle is an S-Swap.");
                            System.out.println("\nSolution: [R2' : [S', R F R']]");
                            System.out.println("Or: R2' S' R F R' S R F' R' R2");
                            break;
                        case "N":
                            System.out.println("Second Target: RB");
                            System.out.println("This cycle is a 4-Mover.");
                            System.out.println("\nSolution: [u L' : [E', L2]]");
                            System.out.println("Or: u L' E' L2 E L2' L u");
                            break;
                        case "O":
                            System.out.println("Second Target: RD");
                            System.out.println("This cycle is an S-Swap.");
                            System.out.println("\nSolution: [S', R F R']");
                            System.out.println("Or: S' R F R' S R F' R'");
                            break;
                        case "P":
                            System.out.println("Second Target: RF");
                            System.out.println("This cycle is a 4-Mover.");
                            System.out.println("\nSolution: [U E L : [E, L2']]");
                            System.out.println("Or: U E L E L2' E' L2 L' E' U'");
                            break;
                        case "Q":
                            System.out.println("Second Target: BU");
                            System.out.println("This cycle is an M-Swap.");
                            System.out.println("\nSolution: [l : [U L U', M']]");
                            System.out.println("Or: l U L U' M' U L' U' M");
                            break;
                        case "R":
                            System.out.println("Second Target: BL");
                            System.out.println("This cycle is an algorithm");
                            System.out.println("\nSolution: L U' L' U' L' U' L U L U");
                            break;
                        case "S":
                            System.out.println("Second Target: BD");
                            System.out.println("This cycle is a 4-Mover.");
                            System.out.println("\nSolution: [U' R' B R' : [S', R2]]");
                            System.out.println("Or: U' R' B R' S' R2 S R2' R B' R U");
                            break;
                        case "T":
                            System.out.println("Second Target: BR");
                            System.out.println("This cycle is a 4-Mover.");
                            System.out.println("\nSolution: U L : [E, L2']");
                            System.out.println("Or: U L E L2' E' L2 L' U'");
                            break;
                        case "U":
                            System.out.println("Second Target: DF");
                            System.out.println("This cycle is an F-Swap.");
                            System.out.println("\nSolution: [L F : [L' S L, F2']]");
                            System.out.println("Or: L F L' S L F2' L' S' L F2 F' L");
                            break;
                        case "V":
                            System.out.println("Second Target: DR");
                            System.out.println("This cycle is a 4-Mover.");
                            System.out.println("\nSolution: [U' : [R2', S']]");
                            System.out.println("Or: U' R2' S' R2 S U");
                            break;
                        case "W":
                            System.out.println("\nLocation: DB");
                            System.out.println("This cycle is an F-Swap.");
                            System.out.println("\nSolution: [D L F' : [L' S L, F2']]");
                            System.out.println("Or: D L F' L' S L F2' L' S' L F2 F L' D'");
                            break;
                        case "X":
                            System.out.println("\nLocation: DL");
                            System.out.println("This cycle cannot be done");
                            break;
                    }
                    break;
        }
    }

