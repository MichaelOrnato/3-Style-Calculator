                            break;
                        case "Q":
                            System.out.println("Second Target: BU");
                            System.out.println("This cycle is an M-Swap.");
                            System.out.println("\nSolution: [r' : [U' R' U, M']]");
                            System.out.println("Or: r' U' R' U M' U' R U M r");
                            break;
                        case "R":
                            System.out.println("Second Target: BL");
                            System.out.println("This cycle is a 4-Mover.");
                            System.out.println("\nSolution: [U' R' : [E', R2]]");
                            System.out.println("Or: U' R' E' R2 E R2' R U");
                            break;
                        case "S":
                            System.out.println("Second Target: BD");
