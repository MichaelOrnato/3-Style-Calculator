/*******************************************************************************
 * Program Name: Commutators
 * Program Description: This program helps learn 3-style edge commutators
 *
 * Program Date Started: 02/20/2021
 * Date Finished:        02/22/2021
 * Program Author: Michael Ornato
 * Change #:        Change Date:        Programmer Name:        Description:
 * ---------        ------------        ---------------         ------------
 ******************************************************************************/
package commutators;

import java.util.*;

public class Commutators {

    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        System.out.println("A commutator works like this: [A B] = A B A' B' ");
        System.out.println("Example: [R U R', M'] = R U R' M' R U' R' M");
        // Prompt user to enter first letter
        System.out.print("\nIn 3-Style Blindfolded Solving, the buffer piece is "
                + "the UF edge piece. \nLook at the UF edge piece, and enter its letter: ");
        String letter1 = input.next();
        letter1 = letter1.toUpperCase();

        // Prompt user to enter second letter:
        System.out.print("What piece is in its spot? ");
        String letter2 = input.next();
        letter2 = letter2.toUpperCase();

        // Create a 'Switch' Statement to get cycle:
        switch (letter1) {
            case "A": 
                System.out.println("\nFirst Target: BU");
                switch (letter2) {
                    case "A": 
                        System.out.println("Second Target: UB");
                        System.out.println("This cycle cannot be done");
                        break;
                    case "B":
                        System.out.println("Second Target: UR");
                        System.out.println("This cycle is a 4-mover. ");
                        System.out.println("Solution: [R2 U' : [S, R2']]");
                        System.out.println("Or: R2 U' S R2' S' R2 U R2");
                        break;
                    case "D":
                        System.out.println("Second Target: UL");
                        System.out.println("This cycle is a 4-mover.");
                        System.out.println("Solution: [L2' U : [S, L2]]");
                        System.out.println("Or: L2' U S L2 S' L2 U' L2");
                        break;
                    case "E":
                        System.out.println("Second Target: LU");
                        System.out.println("This cycle is a 4-mover.");
                        System.out.println("Solution: [U' M U' : [M', U2]]");
                        System.out.println("Or: U' M U' M' U2 M U2 U M' U");
                        break;
                    case "F":
                        System.out.println("Second Target: LF");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("Solution: [U' : [R' E R, U2]]");
                        System.out.println("Or: U' R' E R U2 R' E' R U2 U");
                        break;
                    case "G":
                        System.out.println("Second Target: LD");
                        System.out.println("This cycle is a U-swap.");
                        System.out.println("Solution: [U : [L' E' L, U2']]");
                        System.out.println("Or: U L' E' L U2 L' E L U2");
                        break;
                    case "H":
                        System.out.println("Second Target: LB");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("Solution: [U' : [R E' R', U2]]");
                        System.out.println("Or: U' R E' R' U2 R E R' U2");
                        break;
                    case "J":
                        System.out.println("Second Target: FR");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("Solution: [R' U' : [S, R2']]");
                        System.out.println("Or: R' U' S R2' S' R2 U R");
                        break;
                    case "K":
                        System.out.println("Second Target: FD");
                        System.out.println("This cycle is an E-Swap");
                        System.out.println("Solution: [R' F' : [R U R', E]]");
                        System.out.println("Or: R' F' R U R' E R U' R' E' F R");
                        break;
                    case "L":
                        System.out.println("Second Target: FL");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("Solution: [L U : [S', L2']]");
                        System.out.println("Or: L U S' L2' S L2 U' L'");
                        break;
                    case "M":
                        System.out.println("Second Target: RU");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("Solution: [U M U : [M', U2']]");
                        System.out.println("Or: U M U M' U2' M U2 U' M' U");
                        break;
                    case "N":
                        System.out.println("Second Target: RB");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("Solution: [U' : [L' E L, U2']]");
                        System.out.println("Or: U' L' E L U2' L' E' L U2 U");
                        break;
                    case "O":
                        System.out.println("Second Target: RD");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("Solution: [U' : [R E R', U2]]");
                        System.out.println("Or: U' R E R' U2 R E' R' U2 U");
                        break;
                    case "P":
                        System.out.println("Second Target: RF");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("Solution: [U : [L E' L', U2']]");
                        System.out.println("Or: U L E' L' U2' L E' L' U2 U");
                        break;
                    case "Q":
                        System.out.println("Second Target: BU");
                        System.out.println("This cycle cannot be done.");
                        break;
                    case "R":
                        System.out.println("Second Target: BL");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [L' U' : [L2, S']]");
                        System.out.println("Or: L' U' L2 S' L2' S U L ");
                        break;
                    case "S":
                        System.out.println("Second Target: BD");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [U' R' B R : [S R2']]");
                        System.out.println("Or: U' R' B R S R2' S' R2 R' B' R U");
                        break;
                    case "T":
                        System.out.println("Second Target: BR");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [R U : [R2', S]");
                        System.out.println("Or: R U R2' S R2 S' U' R'");
                        break;
                    case "U":
                        System.out.println("Second Target: DF");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [U2 M']");
                        System.out.println("Or: U2 M' U2' M ");
                        break;
                    case "V":
                        System.out.println("Second Target: DR");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [U' : [S, R2']]");
                        System.out.println("Or: U' S R2' S' R2 U");
                        break;
                    case "W":
                        System.out.println("Second Target: DB");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [M, U2]");
                        System.out.println("Or: M U2 M' U2'");
                        break;
                    case "X":
                        System.out.println("Second Target: DL");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [U : [S', L2]]");
                        System.out.println("Or: U S' L2 S L2' U'");
                        break;
                }
                break;
            case "B":
                System.out.println("\nFirst Target: UR");
                switch (letter2) {
                    case "A":
                        System.out.println("Second Target: UB");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [R2 U' : [R2', S]]");
                        System.out.println("Or: R2 U' R2' S R2 S' U R2'");
                        break;
                    case "B":
                        System.out.println("Second Target: UR");
                        System.out.println("This cycle cannot be done");
                        break;
                    case "D":
                        System.out.println("Second Target: UL");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [M2' U : [M, U2']]");
                        System.out.println("Or: M2' U M U2' M' U2 U' M2");
                        break;
                    case "E":
                        System.out.println("Second Target: LU");
                        System.out.println("This cycle is an S-Swap");
                        System.out.println("\nSolution: [S', L F' L']");
                        System.out.println("Or: S' L F' L' S L F L'");
                        break;
                    case "F":
                        System.out.println("Second Target: LF");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U', R' E R]");
                        System.out.println("Or: U' R' E R U R' E' R");
                        break;
                    case "G":
                        System.out.println("Second Target: LD");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U : [L' E' L, U]]");
                        System.out.println("Or: U L' E' L U L' E L U' U'");
                        break;
                    case "H":
                        System.out.println("Second Target: LB");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U', R E' R']");
                        System.out.println("Or: U' R E' R' U R E R'");
                        break;
                    case "J":
                        System.out.println("Second Target: FR");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [E' : [U', R' E R]]");
                        System.out.println("Or: E' U' R' E R U R' E' R E");
                        break;
                    case "K":
                        System.out.println("Second Target: FD");
                        System.out.println("This cycle is an M-Swap.");
                        System.out.println("\nSolution: [r : [U' R' U, M]]");
                        System.out.println("Or: r U' R' U M U' R U M' r'");
                        break;
                    case "L":
                        System.out.println("Second Target: FL");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U', R E2 R']");
                        System.out.println("Or: U' R E2 R' U R E2 R'");
                        break;
                    case "M":
                        System.out.println("Second Target: RU");
                        System.out.println("This cycle cannot be done.");
                        break;
                    case "N":
                        System.out.println("Second Target: RB");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("Solution: [U : [L' E L, U]]");
                        System.out.println("Or: U L' E L U L' E' L U' U'");
                        break;
                    case "O":
                        System.out.println("Second Target: RD");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U', R E R']");
                        System.out.println("Or: U' R E R' U R E' R'");
                        break;
                    case "P":
                        System.out.println("Second Target: RF");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U : [L E' L', U]]");
                        System.out.println("Or: U L E' L' U L E L' U' U'");
                        break;
                    case "Q":
                        System.out.println("Second Target: BU");
                        System.out.println("This cycle is an M-Swap.");
                        System.out.println("\nSolution: [r' : [U' R U, M']]"); 
                        System.out.println("Or: r' U' R U M' U' R' U M r");
                        break;
                    case "R":
                        System.out.println("Second Target: BL");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U', R' E2 R]");
                        System.out.println("Or: U' R' E2 R U R' E2 R");
                        break;
                    case "S":
                        System.out.println("Second Target: BD");
                        System.out.println("This cycle is an algorithm.");
                        System.out.println("(U M U M')2");
                        System.out.println("Or: U M U M' U M U M'");
                        break;
                    case "T":
                        System.out.println("Second Target: BR");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: u' : [R E' R', U]");
                        System.out.println("Or: u' R E' R' U R E R' U' u");
                        break;
                    case "U":
                        System.out.println("Second Target: DF");
                        System.out.println("This cycle is an F-Swap");
                        System.out.println("\nSolution: [L F : [L' S' L, F2]]");
                        System.out.println("Or: L F L' S' L F2 L' S L F2 F' L'");
                        break;
                    case "V":
                        System.out.println("Second Target: DR");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [R' U R' : [S, R2]]");
                        System.out.println("Or: R' U R' S R2 S' R2' R U' R");
                        break;
                    case "W":
                        System.out.println("Second Target: DB");
                        System.out.println("This cycle is an M-Swap.");
                        System.out.println("\nSolution: [r M' : [U' R' U, M2']]");
                        System.out.println("Or: r M' U' R' U M2' U' R U M2 M r'");
                        break;
                    case "X":
                        System.out.println("Second Target: DL");
                        System.out.println("This cycle is an algorithm.");
                        System.out.println("\nSolution: U' L' U' L U L U L U' L'");
                        break;
                }
                break;
            case "D": 
                System.out.println("First Target: UL");
                switch (letter2) {
                    case "A":
                        System.out.println("Second Target: UB");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [L2' U : [L2, S']]");
                        System.out.println("Or: L2' U L2 S' L2' S U' L2");
                        break;
                    case "B":
                        System.out.println("Second Target: UR");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [M2' U' : [M, U2]]");
                        System.out.println("Or: M2' U' M U2 M' U2' U M2");
                        break;
                    case "D":
                        System.out.println("Second Target: UL");
                        System.out.println("This cycle cannot be done.");
                        break;
                    case "E":
                        System.out.println("Second Target LU");
                        System.out.println("This cycle cannot be done.");
                        break;
                    case "F":
                        System.out.println("Second Target: LF");
                        System.out.println("This cycle is a U-Swap");
                        System.out.println("\nSolution: [U' : [R' E R, U']]");
                        System.out.println("Or: U' R' E R U' R' E' R U U");
                        break;
                    case "G":
                        System.out.println("Second Target: LD");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U, L' E' L]");
                        break;
                    case "H":
                        System.out.println("Second Target: LB");
                        System.out.println("This cycle is a U-Swap");
                        System.out.println("\nSolution: [U' : [R E' R', U']]");
                        System.out.println("Or: U' R E' R' U' R E R' U U");
                        break;
                    case "J":
                        System.out.println("Second Target: FR");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U, L' E2' L]");
                        System.out.println("Or: U L' E2' L U' L' E2 L");
                        break;
                    case "K":
                        System.out.println("Second Target: FD");
                        System.out.println("This cycle is an M-Swap.");
                        System.out.println("\nSolution: [l' : [U L U', M]]");
                        System.out.println("Or: l' U L U' M U L' U' M' l");
                        break;
                    case "L":
                        System.out.println("Second Target: FL");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U' : [R E2 R', U']");
                        System.out.println("Or: U' R E2 R' U' R E2 R' U U");
                        break;
                    case "M":
                        System.out.println("Second Target: RU");
                        System.out.println("This cycle is an S-Swap.");
                        System.out.println("\nSolution: [S, R' F R]");
                        System.out.println("Or: S R' F R S' R' F' R");
                        break;
                    case "N":
                        System.out.println("Second Target: RB");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U, L' E L]");
                        System.out.println("Or: U L' E L U' L' E' L");
                        break;
                    case "O":
                        System.out.println("Second Target: RD");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U' : [R E R', U']]");
                        System.out.println("Or: U' R E R' U' R E' R' U U");
                        break;
                    case "P":
                        System.out.println("Second Target: RF");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U, L E' L']");
                        System.out.println("Or: U L E' L' U' L E L'");
                        break;
                    case "Q":
                        System.out.println("Second Target: BU");
                        System.out.println("This cycle is an M-Swap.");
                        System.out.println("\nSolution: [L : [M, U L' U']]");
                        System.out.println("Or: L M U L' U' M' U L U' L'");
                        break;
                    case "R":
                        System.out.println("Second Target: BL");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U' : [R E2 R', U']]");
                        System.out.println("Or: U' R E2 R' U' R E2 R' U U");
                        break;
                    case "S":
                        System.out.println("Second Target: BD");
                        System.out.println("This cycle is an algorithm.");
                        System.out.println("\nSolution: (U' M U' M')2");
                        System.out.println("Or: U' M U' M' U' M U' M'");
                        break;
                    case "T":
                        System.out.println("Second Target: BR");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [U, L E2' L']");
                        System.out.println("Or: U L E2' L' U' L E2' L'");
                        break;
                    case "U":
                        System.out.println("Second Target: DF");
                        System.out.println("This cycle is an F-Swap.");
                        System.out.println("\nSolution: [R' F' : [R S R', F2]]");
                        System.out.println("Or: R' F' R S R' F2 R S' R' F2' F R");
                        break;
                    case "V":
                        System.out.println("Second Target: DR");
                        System.out.println("This cycle is an Algorithm.");
                        System.out.println("\nSolution: U R U R' U' R' U' R' U R");
                        break;
                    case "W":
                        System.out.println("Second Target: DB");
                        System.out.println("This cycle is an M-Swap.");
                        System.out.println("\nSolution: [l' M' : [U L U', M2']]");
                        System.out.println("Or: l' M' U L U' M2' U L' U' M2 M l");
                        break;
                    case "X":
                        System.out.println("Second Target: DL");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [L U' L : [S', L2']]");
                        System.out.println("Or: L U' L S' L2' S L2 L' U L'");
                        break;
                }
                break;
            case "E": 
                System.out.println("First Target: LU");
                switch (letter2) {
                    case "A":
                        System.out.println("Second Target: UB");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [U' M U : [M', U2]]");
                        System.out.println("Or: U' M U M' U2 M U2' U' M' U'");
                        break;
                    case "B":
                        System.out.println("Second Target: UR");
                        System.out.println("This cycle is an S-Swap.");
                        System.out.println("\nSolution: [L F' L', S']");
                        break;
                    case "D":
                        System.out.println("Second Target: UL");
                        System.out.println("This cycle cannot be done");
                        break;
                    case "E":
                        System.out.println("Second Target: LU");
                        System.out.println("This cycle cannot be done");
                        break;
                    case "F":
                        System.out.println("Second Target: LF");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [S : [U', R' E R]]");
                        System.out.println("Or: S U' R' E R U R' E' R U S'");
                        break;
                    case "G":
                        System.out.println("Second Target: LD");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [l' U L' : [S', L2]]");
                        System.out.println("Or: l' U L' S' L2 S L2' L U' l");
                        break;
                    case "H":
                        System.out.println("Second Target: LB");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [S : [U', R E' R']]");
                        System.out.println("Or: S U' R E' R' U R E R' S'");
                        break;
                    case "J":
                        System.out.println("Second Target: FR");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [M' : [U', R S' R']]");
                        System.out.println("Or: M' U' R S' R' U R S R' M");
                        break;
                    case "K":
                        System.out.println("Second Target: FD");
                        System.out.println("This cycle is an M-Swap.");
                        System.out.println("\nSolution: [l' : [M', U' L U]]");
                        System.out.println("Or: l' M' U' L U M U' L' U l");
                        break;
                    case "L":
                        System.out.println("Second Target: FL");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [L' U2 : [L S L', U]]");
                        System.out.println("Or: L' U2 L S L' U L S' L' U' U2' L'");
                        break;
                    case "M":
                        System.out.println("Second Target: RU");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [M U : [M', U2']]");
                        System.out.println("Or: M U M' U2' M U2 U' M");
                        break;
                    case "N":
                        System.out.println("Second Target: RB");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [L' F' : [E, L2]]");
                        System.out.println("Or: L' F' E L2 E' L2' F L");
                        break;
                    case "O":
                        System.out.println("Second Target: RD");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [R' F R : [S, R2']]");
                        System.out.println("Or: R' F R S R2' S' R2 R' F' R");
                        break;
                    case "P":
                        System.out.println("Second Target: RF");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [R M U : [M', U2]]");
                        System.out.println("Or: R M U' M' U2 M U2' U' M' R'");
                        break;
                    case "Q":
                        System.out.println("Second Target: BU");
                        System.out.println("This cycle is an M-Swap.");
                        System.out.println("\nSolution: [L' : [U' L' U, M']]");
                        System.out.println("Or: L' U' L' U M' U' L U M' L");
                        break;
                    case "R":
                        System.out.println("Second Target: BL");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [U' R F : [R2' E']]");
                        System.out.println("Or: U' R F R2' E' R2 E F' R' U");
                        break;
                    case "S":
                        System.out.println("Second Target: BD");
                        System.out.println("This cycle is an algorithm.");
                        System.out.println("\nSolution: M' : (U M' U M)2");
                        System.out.println("Or: M' U M' U M U M' U M M");
                        break;
                    case "T":
                        System.out.println("Second Target: BR");
                        System.out.println("This cycle is a U-Swap.");
                        System.out.println("\nSolution: [M' : [U', L' E L]]");
                        System.out.println("Or: M' U' L' E L U L' E' L M");
                        break;
                    case "U":
                        System.out.println("Second Target: DF");
                        System.out.println("This cycle is a 4-Mover.");
                        System.out.println("\nSolution: [L' F' : [L2, E']]");
                        System.out.println("Or: L' F' L2 E' L2' E F L");
                        break;
                    case "V":
                        System.out.println("Second Target: DR");
                        System.out.println("This cycle is an S-Swap.");
                        System.out.println("\nSolution: [L2 : [L' F' L, S]]");
                        System.out.println("Or: L2 L' F' L S L' F L S' L2");
