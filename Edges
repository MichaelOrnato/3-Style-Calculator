                            System.out.println("Or: D S' R F R' S R F' R' D'");
                            break;
                        case "L":
                            System.out.println("\nLocation: FL");
                            System.out.println("This cycle is an M-Swap.");
                            System.out.println("\nSolution: [U L' U', M2']");
                            System.out.println("Or: U L' U' M2' U L U' M2");
                            break;
                        case "M":
