                            System.out.println("Or: M' U R U' M2' U R' U' M2 M");
                            break;
                        case "Q":
                            System.out.println("Second Target: BU");
                            System.out.println("This cycle is an S-Swap.");
                            System.out.println("\nSolution: [U : [R B R', S]]");
                            System.out.println("Or: U R B R' S R B' R' S' U'");
                            break;
                        case "R":
                            System.out.println("Second Target: BL");
                            System.out.println("This cycle is an M-Swap.");
                            System.out.println("\nSolution: [U L U', M2']");
                            System.out.println("Or: U L U' M2' U L' U' M2");
